from pithy import *
import json 
from urllib import urlopen as uo

durr = []

for i in range(100):
    d = []
    for x in uo("http://localhost:9000/data").read().strip().split(","):
        try:
            d.append(int(x))
        except:
            print x
    durr.append(d)
    time.sleep(0.25)
    
imshow(durr,aspect="auto")
showme()
clf()